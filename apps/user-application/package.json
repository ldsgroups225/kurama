{
  "name": "kurama-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "build:production": "vite build && pnpm run perf:check-bundles",
    "build:analyze": "vite build && open dist/stats.html",
    "analyze": "vite build --mode production && open dist/stats.html",
    "deploy": "pnpm run build:production && wrangler deploy",
    "serve": "vite preview",
    "cf-typegen": "wrangler types --env-interface Env",
    "typecheck": "tsc --noEmit",
    "perf:check": "tsx scripts/check-performance.ts",
    "perf:check-bundles": "tsx scripts/check-bundle-size.ts",
    "perf:validate": "pnpm run build && pnpm run perf:check-bundles",
    "validate:optimizations": "tsx scripts/validate-optimizations.ts",
    "validate:all": "pnpm run typecheck && pnpm run build && pnpm run validate:optimizations",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kurama/data-ops": "workspace:^",
    "@polar-sh/sdk": "^0.34.17",
    "@polar-sh/tanstack-start": "^0.1.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.15",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.133.22",
    "@tanstack/react-router-devtools": "^1.133.22",
    "@tanstack/react-router-ssr-query": "^1.133.22",
    "@tanstack/react-start": "^1.133.22",
    "better-auth": "^1.3.29",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "jotai": "^2.15.1",
    "lucide-react": "^0.476.0",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.15",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.18.12",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/remark-prism": "^1.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "jsdom": "^26.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "7.1.2",
    "vitest": "^3.2.4",
    "web-vitals": "^4.2.4",
    "wrangler": "^4.44.0"
  }
}
